	Issn = {01429612},
	Journal = {Biomaterials},
	Keywords = {Calcium phosphate cement,Injectability,Mechanical properties,Zeta-potential},
	Month = {may},
	Number = {11},
	Pages = {2187--2195},
	Title = {{Ionic modification of calcium phosphate cement viscosity. Part I: hypodermic injection and strength improvement of apatite cement}},
