	Keywords = {Humans,Incidence,Kyphosis,Kyphosis: physiopathology,Kyphosis: surgery,Orthopedic Procedures,Orthopedic Procedures: adverse effects,Palliative Care,Spinal Fractures,Spinal Fractures: epidemiology,Spinal Fractures: etiology,Spine,Spine: surgery,Treatment Outcome},
