@article{mosekilde1994effect,
	Abstract = {The aim of the study was to assess the long term effect of the antiresorptive agents estrogen and salmon calcitonin, the anabolic drug PTH, and combination therapy on vertebral bone mass and bone biomechanical competence in aged osteopenic ovariectomized (OVX) rats. Seventy-nine 1-yr-old retired breeder Wistar rats were randomized into seven groups: 1) sham-operated, 2) OVX, 3) OVX plus estrogen, 4) OVX plus salmon calcitonin, 5) OVX plus human (h) PTH-(1-38), 6) OVX plus hPTH-(1-38) and estrogen, and finally, 7) OVX plus hPTH-(1-38) and calcitonin. Treatment regimens were initiated 8 weeks after ovariectomy and continued for 24 weeks. Whole body bone mass was measured by dual photon absorptiometry at intervals of 4 weeks and at death. Changes in spinal bone mass (BMC), biomechanical competence, and structure were assessed from the lumbar vertebral bodies. The results revealed a protective effect of both estrogen and calcitonin on ovariectomy-induced loss of whole body bone mass, but only estrogen had a significant effect on spinal BMC. hPTH-(1-38) monotherapy increased vertebral bone mass (BMC and ash content), size, and biomechanical parameters 20-80{\%} over OVX levels. A more rapid effect on vertebral bone mass was seen when hPTH-(1-38) was combined with estrogen or salmon calcitonin. The study has shown that in aged OVX osteopenic rats, hPTH-(1-38) therapy increases vertebral bone mass and bone quality and also maintains trabecular connectivity. Both estrogen and calcitonin reduce the ovariectomy-induced bone loss, but cannot restore bone mass to sham-operated levels. On the basis of this study, it is concluded that PTH therapy seems to hold promise in the management of postmenopausal and age-related osteoporosis.},
	Author = {Mosekilde, L and Danielsen, C C and Gasser, J},
	Doi = {10.1210/endo.134.5.8156913},
	Isbn = {0013-7227 (Print)$\backslash$r0013-7227 (Linking)},
	Journal = {Endocrinology},
	Keywords = {*Aging,Absorptiometry, Photon,Animals,Biomechanical Phenomena,Bone Resorption,Bone and Bones/drug effects/*physiology,Calcitonin/*pharmacology,Estradiol/*pharmacology,Female,Microscopy, Electron, Scanning,Ovariectomy,Parathyroid Hormone/*pharmacology,Peptide Fragments/*pharmacology,Rats,Rats, Wistar,Spine/physiology},
	Number = {5},
	Pages = {2126--2134},
	Pmid = {8156913},
